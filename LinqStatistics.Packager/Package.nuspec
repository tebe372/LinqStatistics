<?xml version="1.0"?>
<package >
  <metadata>
    <id>LinqStatistics</id>
    <version>2.0.0</version>
    <title>LinqStatistics</title>
    <authors>dkackman</authors>
    <owners></owners>
    <description>
      Extension methods that add additional simple statiistical calculations to the built in sum, min, max, average etc.
      Includes Covariance, Median, Mode, Pearson's Chi Squared, Range, Standard Deviation (sample and population), Variance (sample and population), CountEach, Histograms, Root Mean Square, Least Squares and Kurtosis
    </description>
    <releaseNotes>
      1.1.0
      - Made code comments, argument checking and implementations consistent
      2.0.0
      - Added MinMax (calculate minimum and maximum in a single enumeration)
      - Improve performance of Histogram and Range by eliminating multiple enumeration
      - Some interface changing refactoring (Range and Bin classes)
    </releaseNotes>
    <summary>
    </summary>
    <language>en-US</language>
    <projectUrl>https://github.com/dkackman/LinqStatistics</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/dkackman/DynamicRestProxy/master/nugeticon.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>http://opensource.org/licenses/Apache-2.0</licenseUrl>
    <copyright>Copyright Don Kackman 2013</copyright>
    <dependencies>
        <group targetFramework="net40">
        </group>
    </dependencies>
    <references></references>
    <tags>Linq Statistics stats</tags>
  </metadata>
  <files>
    <file src="..\LinqStatistics\bin\Release\LinqStatistics.dll" target="net40" />
    <file src="..\LinqStatistics\bin\Release\LinqStatistics.xml" target="net40" />
    <file src="..\LinqStatistics\bin\Release\LinqStatistics.pdb" target="net40" />

    <file src="..\DynamicRestProxy.Portable\*.cs" target="src" />
  </files>
</package>